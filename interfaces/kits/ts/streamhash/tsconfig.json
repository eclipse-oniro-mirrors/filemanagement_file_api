{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "rootDir": "./src",
        "outDir": "./out", /* Specify an output folder for all emitted files. */
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitThis": false,
    }
}